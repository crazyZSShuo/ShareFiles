{
  "name": "sharefilescf-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
